{
  "name": "magic-transporters-task",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npx tsc-alias -p tsconfig.json -f -fe .js",
    "start": "tsx src/index.ts",
    "dev": "tsx src/index.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:mongo": "docker compose up mongo -d",
    "dev:watch": "nodemon --watch src --ext ts --exec \"tsx src/index.ts\"",
    "test": "jest --forceExit",
    "test:e2e": "jest --testPathPattern=e2e --forceExit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "mongoose": "^8.0.0",
    "swagger-ui-express": "^5.0.0",
    "yaml": "^2.3.4",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0",
    "@types/supertest": "^7.2.0",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.0.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
